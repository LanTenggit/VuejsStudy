@{ 
    Layout = null;
}
<html>
<head>
    <title>axios(ajax)</title>
    <script src="~/Content/vue2.4.2.min.js"></script>
    <script src="~/Content/axios.min.js"></script>
    <style type="text/css">
        * {
        padding:0px;
        margin:0px;
        }
    </style>



</head>

<body>
    <div>
        <div id="app">


            {{ info }}


        </div>

       
    </div>
  
    <script type="text/javascript"> 
            new Vue({
                el: '#app',
                data() {
                    return {
                        info: null
                    }
                },
                mounted() {
                    axios
                        .get('../VueAjaxAxios/GetList')
                        .then(response => (this.info = response.data))
                        .catch(function (error) { // 请求失败处理
                            console.log(error);
                        });
                }
            })
    </script>



  

</body>



</html>